{
    "port": 8080,
    "env": "development",
    "useProcessEnv": true,
    "processEnvVar": "NODE_ENV",
    "processPortVar": "PORT",
    "logEnabled": true,
    "debugEnabled": true,
    "logFilesPath": "./logs/",
    "passwordHashAlgorithm": "md5", // bcrypt is also available with additional dependency - https://www.npmjs.com/package/bcrypt
    "facebookAuth": {
        "clientID": "your-secret-clientID-here",
        "clientSecret": "your-client-secret-here",
        "callbackURL": "http://localhost:8080/auth/facebook/callback"
    },
    "twitterAuth": {
        "consumerKey": "your-consumer-key-here",
        "consumerSecret": "your-client-secret-here",
        "callbackURL": "http://localhost:8080/auth/twitter/callback"
    },
    "googleAuth": {
        "clientID": "your-secret-clientID-here",
        "clientSecret": "your-client-secret-here",
        "callbackURL": "http://localhost:8080/auth/google/callback"
    },
    "session": {
        "store": "memory",
        "key": "sid",
        "secret": "sessionsecretkey"
    },
    "redis": {
        "host": "localhost",
        "port": 6379
    }
}