{
  "name": "express-template-facade",
  "version": "1.0.5",
  "description": "Website template (skeleton) based on Express.js 4, Vue.js and Vuetify 2",
  "author": "NordicSoft",
  "license": "MIT",
  "private": true,
  "main": "server/index.js",
  "commit": "${commit}",
  "backup": "${backup}",
  "scripts": {
    "lint": "npx eslint --fix --ext .js .",
    "forever-start": "npx cross-env PORT=8082 RUNNING_FOREVER=1 npx cross-var npx forever start -a -l $INIT_CWD/logs/forever.log --uid express-template-facade index.js",
    "forever-stop": "npx forever stop express-template-facade",
    "build": "npx gulp build",
    "deploy": "npx gulp deploy",
    "dev": "npx concurrently npm:lint \"npx nodemon\" gulp --raw --kill-others-on-fail",
    "start": "node server/index.js"
  },
  "_moduleAliases": {
    "@root": ".",
    "@server": "server",
    "@lib": "server/lib",
    "@api": "server/lib/api",
    "@config": "server/config.js",
    "@logger": "server/lib/logger.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "animate.css": "^3.7.2",
    "aos": "^2.3.4",
    "aws-sdk": "^2.635.0",
    "axios": "^0.19.2",
    "body-parser": "1.x.x",
    "bootstrap": "^4.4.1",
    "bootstrap-sass": "^3.4.1",
    "clean-css": "^4.2.3",
    "clone": "2.x.x",
    "compression": "^1.7.4",
    "connect-redis": "^4.0.4",
    "cookie": "^0.4.0",
    "cookie-parser": "1.x.x",
    "cors": "^2.8.5",
    "dotenv-defaults": "^1.1.1",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-hbs": "^2.3.1",
    "express-session": "1.x.x",
    "jquery": "^3.4.1",
    "jquery-validation": "^1.19.1",
    "justifiedGallery": "^3.7.0",
    "lodash": "^4.17.15",
    "method-override": "3.x.x",
    "minimatch": "^3.0.4",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.4",
    "page": "^1.11.5",
    "passport": "0.4.x",
    "passport-facebook": "3.x.x",
    "passport-google-oauth": "2.x.x",
    "passport-local": "1.x.x",
    "passport-twitter": "1.x.x",
    "redis": "^3.0.2",
    "serve-favicon": "2.x.x",
    "slick-carousel": "^1.8.1",
    "socket.io": "2.3.x",
    "winston": "3.x.x"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "autoprefixer": "^9.7.4",
    "browser-sync": "^2.26.7",
    "concurrently": "^5.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.1",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-declare": "^0.3.0",
    "gulp-file": "^0.4.0",
    "gulp-postcss": "^8.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-touch-fd": "^2.0.0",
    "gulp-uglify-es": "^2.0.0",
    "minimist": "^1.2.0",
    "popper.js": "^1.16.1",
    "rollup": "^2.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-sass": "^1.2.2",
    "sass": "^1.26.2",
    "uglify-js": "^3.8.0"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "assets/*",
      "js/*",
      "scss/*",
      "gulpfile.js"
    ],
    "ext": "js,json,html"
  }
}
