{
  "name": "express-template",
  "version": "1.0.2",
  "description": "Website template (skeleton) based on Express.js 4, Vue.js and Vuetify 2",
  "author": "NordicSoft",
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "commit": "${commit}",
  "backup": "${backup}",
  "scripts": {
    "lint": "npx eslint --ext .js,.vue .",
    "lint-fix": "npm run lint -- --fix",
    "forever-start": "cross-env PORT=8080 RUNNING_FOREVER=1 cross-var forever start -a -l $INIT_CWD/logs/forever.log --uid express-template index.js",
    "forever-stop": "forever stop express-template",
    "build": "npx gulp build",
    "deploy": "npx gulp deploy",
    "dev": "npm run lint && nodemon",
    "mkdir-files": "mkdir www/files && mkdir www/files/uploads",
    "mkdir-gallery": "mkdir www/files/gallery && mkdir www/files/gallery/photos && mkdir www/files/gallery/photosets && mkdir www/files/gallery/trash",
    "mkdir": "npm run mkdir-files && npm run mkdir-gallery",
    "start": "npm run mkdir && node index.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "animate.css": "^3.7.2",
    "aos": "^2.3.4",
    "async": "3.x.x",
    "aws-sdk": "^2.510.0",
    "axios": "^0.19.0",
    "body-parser": "1.x.x",
    "bootstrap": "^4.3.1",
    "bootstrap-sass": "^3.4.1",
    "clean-css": "^4.2.1",
    "clone": "2.x.x",
    "compression": "^1.7.4",
    "connect-redis": "^3.4.1",
    "cookie": "^0.4.0",
    "cookie-parser": "1.x.x",
    "cors": "^2.8.5",
    "crc-32": "^1.2.0",
    "cross-env": "^5.2.0",
    "cross-var": "^1.1.0",
    "dotenv-defaults": "^1.0.2",
    "errorhandler": "^1.5.1",
    "escape-string-regexp": "^2.0.0",
    "express": "^4.17.1",
    "express-hbs": "^2.1.2",
    "express-session": "1.x.x",
    "google-maps-api-loader": "^1.1.1",
    "jquery": "^3.4.1",
    "jquery-validation": "^1.19.1",
    "justifiedGallery": "^3.7.0",
    "method-override": "3.x.x",
    "minimatch": "^3.0.4",
    "mongodb": "^3.2.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.0",
    "page": "^1.11.4",
    "passport": "0.4.x",
    "passport-facebook": "3.x.x",
    "passport-google-oauth": "2.x.x",
    "passport-local": "1.x.x",
    "passport-twitter": "1.x.x",
    "popper.js": "^1.15.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-vue": "^5.0.1",
    "serve-favicon": "2.x.x",
    "shush": "^1.0.0",
    "slick-carousel": "^1.8.1",
    "socket.io": "2.2.x",
    "uuid": "3.x.x",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10",
    "vuetify": "^2.1.7",
    "vuetify-file-browser": "^1.0.2",
    "vuetify-file-browser-server": "^1.0.2",
    "vuex": "^3.1.1",
    "winston": "3.x.x",
    "xdate": "0.x.x",
    "xmp-js": "0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@mdi/font": "^4.2.95",
    "autoprefixer": "^9.6.0",
    "browser-sync": "^2.26.7",
    "cssnano": "^4.1.10",
    "eslint": "^6.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-declare": "^0.3.0",
    "gulp-file": "^0.4.0",
    "gulp-postcss": "^8.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-touch-fd": "^2.0.0",
    "gulp-uglify-es": "^1.0.4",
    "minimist": "^1.2.0",
    "rollup": "^1.16.2",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-css-only": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-sass": "^1.2.2",
    "sass": "^1.22.10",
    "uglify-js": "^3.6.0"
  },
  "nodemonConfig": {
    "ignore": [
      "data/*",
      "logs/*",
      "www/*",
      "assets/*",
      "views/app/templates/*",
      "gulpfile.js"
    ],
    "ext": "js,json,html"
  }
}
